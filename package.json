{
  "name": "projeto-autoral",
  "version": "1.0.0",
  "description": "Esse projeto autoral com objetivo de ser um portofólio",
  "main": "index.js",
  "scripts": {
    "build": "ttsc --project tsconfig.build.json",
    "start": "node dist/server.js",
    "migration:run": "npx prisma migrate deploy",
    "seed": "npx prisma db seed",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "dev:migration:run": "npx prisma migrate deploy",
    "dev:migration:generate": "npx prisma migrate dev",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --watchAll --passWithNoTests --runInBand"
  },
  "keywords": [
    "Accountability",
    "Tecnologias cívicas",
    "Gestão Pública",
    "Dados Abertos",
    "back-end",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@prisma/client": "^4.6.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.4.6"
  }
}
